{
  "name": "kevinselwyn.com",
  "version": "1.0.0",
  "description": "Personal website that builds/deploys to kevinselwyn.github.io",
  "main": "index.js",
  "repository": "https://github.com/kevinselwyn/kevinselwyn.com",
  "author": "Kevin Selwyn <kevinselwyn@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "dev": "hugo --config config.toml,dev.toml serve --buildDrafts --port 1234",
    "serve": "cd public && python -m SimpleHTTPServer 1234",
    "build": "hugo",
    "package": "yarn run package:html && yarn run package:xml && yarn run package:js && yarn run package:css && yarn run package:resume",
    "package:html": "find public -name '*.html' -exec html-minifier --collapse-whitespace --minify-css inline --minify-js inline {} --output {} \\;",
    "package:xml": "find public -name '*.xml' -exec html-minifier --collapse-whitespace --minify-css inline --minify-js inline {} --output {} \\;",
    "package:js": "ENTRY=`find static/js -name '*.js' ! -name 'react*.min.js' ! -name 'prop-types*'` NODE_ENV=production webpack --mode=production",
    "package:css": "find public/css -name '*.css' -exec sh -c \"cat {} | cssmin > {}.tmp && mv {}.tmp {}\" \\;",
    "package:resume": "NODE_ENV=production webpack --mode=production --config resume-ssr.webpack.config.js && node resume-ssr.min.js",
    "deploy": "./deploy.sh",
    "lint": "eslint `find static/js -name '*.js' ! -name 'react*.min.js' ! -name 'prop-types*'`"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "cssmin": "^0.4.3",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "html-minifier": "^3.5.21",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
